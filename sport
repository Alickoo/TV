import re,requests
def GetLSProData(page_data,Cookie_Jar,m):#streamwatcher2013
  list=re.findall('<h2>(?s)(.*?)id=.comments',page_data)[0]
  gtitle=re.findall('\d+:\d+.+?&.+?>([^<]+)',list)
  for t in gtitle:
     title=t.title().strip()
     list=list.replace(t,title)
  data=re.findall('(\d+:\d+).+?>(.*?)\s*&.+?>([^<]+).+?href="([^"]+)',list)
  return data
]]></expres>
